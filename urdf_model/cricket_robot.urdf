<?xml version="1.0"?>
<robot name="origins">
  <material name="yellow">
    <color rgba="0.8 0.8 0 1"/>
  </material>

	<material name="red">
		<color rgba="1 0 0 1"/>
	</material>

  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.41 0.36 0.06"/>
      </geometry>
      <material name="red"/>
    </visual>
  </link>

  <!-- ##### RIGHT SIDE ##### -->
  <link name="right_box">
    <visual>
      <geometry>
        <box size="0.1 0.07 0.06"/>
      </geometry>
      <!-- <origin rpy="0 0 0" xyz="0 0.215 0.0"/> -->
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>

  <joint name="base_to_right_box" type="fixed">
    <parent link="base_link"/>
    <child link="right_box"/>
    <origin xyz="0 0.215 0.0"/>
  </joint>

  <link name="right_cylinder">
		<visual name="right_cylinder">
			<origin rpy="0 1.57075 0" xyz="0 0.0 0"/>
			<geometry>
				<cylinder radius="0.03" length="0.1"/>
			</geometry>
			<material name="red"/>
		</visual>
	</link>

 	<joint name="right_box-cylinder" type="fixed">
		<parent link="right_box"/>
		<child link="right_cylinder"/>	 
    <origin xyz="0.0 0.035 0.0" rpy="0.0 0.0 0.0"/>
	</joint>

  <!-- ##### LEFT SIDE ##### -->
  <link name="left_box">
		<visual>
			<geometry>
				<box size="0.1 0.07 0.06"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0 -0.0 0.0"/>
			<material name="red"/>
		</visual>
	</link>

	<joint name="body_to_left_box" type="fixed">
		<parent link="base_link"/>
		<child link="left_box"/>
		<origin xyz="0.0 -0.215 0.0"/>		
	</joint>

	<link name="left_cylinder">
