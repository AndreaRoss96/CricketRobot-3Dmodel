  </link>

  <joint name="frl_shoulder-body" type="fixed">
    <origin xyz="0.12 0.0 0.0" rpy="0 0 0.0"/>
    <parent link="right_cylinder"/>
    <child link="frl_shoulder_cylinder"/>
  </joint>
  
  <link name="frl_shoulder_box">
