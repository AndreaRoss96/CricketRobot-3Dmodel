	</link>

	<joint name="left_box-cylinder" type="fixed">
		<parent link="left_box"/>
		<child link="left_cylinder"/>	 
    <origin xyz="0.0 -0.035 0.0"/>
	</joint>

  <!-- ##### FRONT RIGHT LEG ##### -->
